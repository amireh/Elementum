<?xml version="1.0" ?>
<GUILayout>
<!--<Window Type="DefaultGUISheet" Name="Elementum/Combat/Root">-->
  <Window Type="DefaultGUISheet" Name="Elementum/Combat">
    <Property Name="Visible" Value="True" />
    <Property Name="MousePassThroughEnabled" Value="True" />
  	<Property Name="Alpha" Value="1" />
  	<Property Name="InheritsAlpha" Value="False" />
  	<Property Name="UnifiedMaxSize" Value="{{1,0},{1,0}}" />
  	<Property Name="UnifiedAreaRect" Value="{{0,0},{0,0},{1,0},{1,0}}" />

    <Window Type="TaharezLook/Button" Name="Elementum/Combat/Reload" >
      <Property Name="Visible" Value="False" />
      <Property Name="Text" Value="Reload UI" />
      <Property Name="UnifiedPosition" Value="{{0.8,0},{0.1,0}}" />
      <Property Name="UnifiedSize" Value="{{0,120},{0,35}}" />
      <Property Name="Disabled" Value="False" />
      <Event Name="Clicked" Function="Pixy.UI.Combat.reload" />
    </Window>
    <Window Type="TaharezLook/Button" Name="Elementum/Combat/ReloadScripts" >
      <Property Name="Visible" Value="False" />
      <Property Name="Text" Value="Reload Scripts" />
      <Property Name="UnifiedPosition" Value="{{0.8,-20},{0.1,40}}" />
      <Property Name="UnifiedSize" Value="{{0,160},{0,35}}" />
      <Property Name="Disabled" Value="False" />
      <Event Name="Clicked" Function="Pixy.Combat.reloadScripts" />
    </Window>

  <!-- Turn panel -->
  <Window Type="TaharezLook/StaticText" Name="Elementum/Combat/Effects/Turns/Menu">
    <Property Name="UnifiedAreaRect" Value="{{0,0},{0,0},{1,0},{0.025.0}}" />
    <Property Name="Visible" Value="True" />
    <Property Name="HorizontalAlignment" Value="Left" />
    <Property Name="VerticalAlignment" Value="Top" />
    <Property Name="BackgroundEnabled" Value="False" />
    <Property Name="FrameEnabled" Value="False" />

    <Window Type="TaharezLook/StaticImage" Name="Elementum/Combat/Effects/Turns/BG">
      <Property Name="AlwaysOnTop" Value="False" />
      <Property Name="BackgroundEnabled" Value="False" />
      <Property Name="FrameEnabled" Value="False" />
      <Property Name="UnifiedAreaRect" Value="{{0,0},{0,0},{1,0},{1,0}}" />
      <Property Name="Image" Value="set:HUDs image:DarkBG" />
    </Window>
    <Window Type="TaharezLook/StaticText" Name="Elementum/Combat/Effects/Turns/Text">
      <Property Name="Visible" Value="True" />
      <Property Name="BackgroundEnabled" Value="False" />
      <Property Name="FrameEnabled" Value="False" />
      <Property Name="HorizontalAlignment" Value="Centre" />
      <Property Name="HorzFormatting" Value="HorzCentred" />
      <Property Name="VerticalAlignment" Value="Centre" />
    </Window> <!-- Player end turn button -->

  </Window> <!-- Menu bar -->

  <!-- Tooltip panel -->
  <Window Type="TaharezLook/StaticText" Name="Elementum/Combat/Containers/Tooltip">
    <Property Name="UnifiedAreaRect" Value="{{0,0},{0,0},{0.2,0},{1,0}}" />
<!--
    <Property Name="UnifiedAreaRect" Value="{{0,0},{0.6,0},{0.52,0},{0.8,0}}" />
-->
    <Property Name="Visible" Value="True" />
    <Property Name="HorizontalAlignment" Value="Left" />
    <Property Name="VerticalAlignment" Value="Top" />
    <Property Name="BackgroundEnabled" Value="False" />
    <Property Name="FrameEnabled" Value="False" />

    <Window Type="TaharezLook/StaticImage" Name="Elementum/Combat/Containers/Tooltip/BG">
      <Property Name="AlwaysOnTop" Value="False" />
      <Property Name="BackgroundEnabled" Value="False" />
      <Property Name="FrameEnabled" Value="False" />
      <Property Name="UnifiedAreaRect" Value="{{0,0},{0,0},{1,0},{1,0}}" />
      <Property Name="Image" Value="set:HUDs image:DarkBG" />
    </Window>
    <Window Type="TaharezLook/StaticText" Name="Elementum/Combat/Labels/Tooltip">
      <Property Name="Font" Value="Alice-12" />
      <Property Name="Visible" Value="True" />
      <Property Name="BackgroundEnabled" Value="False" />
      <Property Name="FrameEnabled" Value="False" />
      <Property Name="HorizontalAlignment" Value="Left" />
      <Property Name="HorzFormatting" Value="WordWrapLeftAligned" />
      <Property Name="VertFormatting" Value="TopAligned" />
      <Property Name="VerticalAlignment" Value="Top" />
    </Window> <!-- Player end turn button -->

  </Window> <!-- Menu bar -->

	<!-- player Spell panel -->
	<Window Type="TaharezLook/ScrollablePane" Name="Elementum/Combat/SpellPanel/Player">
	  <Property Name="Visible" Value="True" />
		<Property Name="AlwaysOnTop" Value="True" />
		<Property Name="Alpha" Value="1" />
    <Property Name="HorizontalAlignment" Value="Centre" />
<!--
    <Property Name="VerticalAlignment" Value="Middle" />
-->
    <Property Name="UnifiedAreaRect" Value="{{0,0},{0.85,0},{0.52,0},{1,0}}" />
<!--
    <Property Name="ContentPaneAutoSized" Value="False" />
-->

    <Window Type="TaharezLook/StaticImage" Name="Elementum/Combat/SpellPanel/Player/BG">
      <Property Name="AlwaysOnTop" Value="False" />
      <Property Name="BackgroundEnabled" Value="False" />
      <Property Name="FrameEnabled" Value="false" />
      <Property Name="UnifiedAreaRect" Value="{{0,0},{0,0},{1,0},{1,0}}" />
      <Property Name="Image" Value="set:HUDs image:Hand2" />
      <Property Name="HorizontalAlignment" Value="Centre" />
    </Window>
    <Window Type="HorizontalLayoutContainer" Name="Elementum/Combat/SpellPanel/Player/Hand">
      <Property Name="Visible" Value="True" />
      <Property Name="AlwaysOnTop" Value="True" />
      <Property Name="HorizontalAlignment" Value="Centre" />
      <Property Name="VerticalAlignment" Value="Centre" />
    </Window>
  </Window>



	<Window Type="TaharezLook/Button" Name="Elementum/Combat/Events/MainMenu" >
		<Property Name="Text" Value="Main Menu" />
		<Property Name="UnifiedSize" Value="{{0.1,0},{0.05,0}}" />
		<Property Name="UnifiedPosition" Value="{{0.8,0},{0.65,0}}" />
		<Property Name="Visible" Value="False" />
		<Property Name="Disabled" Value="False" />
	</Window>

  <!-- match score -->
  <Window Type="TaharezLook/StaticText" Name="Elementum/Combat/Containers/Victory">
    <Property Name="MousePassThroughEnabled" Value="True" />
    <Property Name="UnifiedAreaRect" Value="{{0,0},{0.4,0},{1,0},{0.6.0}}" />
    <Property Name="Visible" Value="False" />
    <Property Name="HorizontalAlignment" Value="Left" />
    <Property Name="VerticalAlignment" Value="Top" />
    <Property Name="BackgroundEnabled" Value="False" />
    <Property Name="FrameEnabled" Value="False" />

    <Window Type="TaharezLook/StaticImage" Name="Elementum/Combat/Containers/Victory/BG">
      <Property Name="AlwaysOnTop" Value="False" />
      <Property Name="BackgroundEnabled" Value="False" />
      <Property Name="FrameEnabled" Value="False" />
      <Property Name="UnifiedAreaRect" Value="{{0,0},{0,0},{1,0},{1,0}}" />
      <Property Name="Image" Value="set:HUDs image:DarkBG" />
    </Window>
    <Window Type="TaharezLook/StaticText" Name="Elementum/Combat/Text/Victory">
      <Property Name="Font" Value="Alice" />
      <Property Name="Visible" Value="True" />
      <Property Name="BackgroundEnabled" Value="False" />
      <Property Name="FrameEnabled" Value="False" />
      <Property Name="HorizontalAlignment" Value="Centre" />
      <Property Name="HorzFormatting" Value="HorzCentred" />
      <Property Name="VerticalAlignment" Value="Centre" />
      <Property Name="Text" Value="Victory!" />
      <Property Name="UnifiedAreaRect" Value="{{0,0},{0,0},{1,0},{1,0}}" />
    </Window>
  </Window> <!-- Match score -->


  <!-- temporary text -->
  <Window Type="TaharezLook/StaticText" Name="Elementum/Combat/Containers/Message">
    <Property Name="MousePassThroughEnabled" Value="True" />
    <Property Name="UnifiedAreaRect" Value="{{0,0},{0.4,0},{1,0},{0.6.0}}" />
    <Property Name="Visible" Value="False" />
    <Property Name="HorizontalAlignment" Value="Left" />
    <Property Name="VerticalAlignment" Value="Top" />
    <Property Name="BackgroundEnabled" Value="False" />
    <Property Name="FrameEnabled" Value="False" />

    <Window Type="TaharezLook/StaticImage" Name="Elementum/Combat/Containers/TempTxt/BG">
      <Property Name="AlwaysOnTop" Value="False" />
      <Property Name="BackgroundEnabled" Value="False" />
      <Property Name="FrameEnabled" Value="False" />
      <Property Name="UnifiedAreaRect" Value="{{0,0},{0,0},{1,0},{1,0}}" />
      <Property Name="Image" Value="set:HUDs image:DarkBG" />
    </Window>

    <Window Type="TaharezLook/StaticText" Name="Elementum/Combat/Text/Message">
      <Property Name="Font" Value="Alice" />
      <Property Name="Visible" Value="True" />
      <Property Name="BackgroundEnabled" Value="False" />
      <Property Name="FrameEnabled" Value="False" />
      <Property Name="HorizontalAlignment" Value="Centre" />
      <Property Name="HorzFormatting" Value="HorzCentred" />
      <Property Name="VerticalAlignment" Value="Centre" />
      <Property Name="Text" Value="Victory!" />
      <Property Name="UnifiedAreaRect" Value="{{0,0},{0,0},{1,0},{1,0}}" />
    </Window>
  </Window> <!-- temporary text -->

  <!-- error message -->
  <Window Type="TaharezLook/StaticText" Name="Elementum/Combat/Containers/Error">
    <Property Name="MousePassThroughEnabled" Value="True" />
    <Property Name="UnifiedAreaRect" Value="{{0.4,0},{0.025,0},{0.6,0},{0.4.0}}" />
    <Property Name="Visible" Value="False" />
    <Property Name="HorizontalAlignment" Value="Left" />
    <Property Name="VerticalAlignment" Value="Top" />
    <Property Name="BackgroundEnabled" Value="False" />
    <Property Name="FrameEnabled" Value="False" />

    <Window Type="TaharezLook/StaticText" Name="Elementum/Combat/Text/Error">
      <Property Name="Font" Value="Alice-12" />
      <Property Name="Visible" Value="True" />
      <Property Name="BackgroundEnabled" Value="False" />
      <Property Name="FrameEnabled" Value="False" />
      <Property Name="HorizontalAlignment" Value="Centre" />
      <Property Name="HorzFormatting" Value="HorzCentred" />
      <Property Name="VerticalAlignment" Value="Centre" />
      <Property Name="Text" Value="Victory!" />
      <Property Name="UnifiedAreaRect" Value="{{0,0},{0,0},{1,0},{1,0}}" />
      <Property Name="ClippedByParent" Value="False" />
    </Window>
  </Window> <!-- temporary text -->
</Window>
</GUILayout>
